{
  "framework": "eleventy",
  "buildCommand": "npm run build:prod",
  "outputDirectory": "_site",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "github": {
    "silent": true,
    "enabled": true
  },
  "regions": ["sfo1"],
  "env": {
    "NODE_ENV": "production",
    "ELEVENTY_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/search",
      "destination": "/search.html"
    },
    {
      "source": "/movie/(.*)",
      "destination": "/movie/[slug]/index.html"
    },
    {
      "source": "/series/(.*)",
      "destination": "/series/[slug]/index.html"
    },
    {
      "source": "/anime/(.*)",
      "destination": "/anime/[slug]/index.html"
    },
    {
      "source": "/watch/(.*)",
      "destination": "/watch/[slug]/index.html"
    },
    {
      "source": "/show/(.*)",
      "destination": "/show/[slug]/index.html"
    },
    {
      "source": "/(.*)/$",
      "destination": "/$1/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/search/",
      "destination": "/search.html",
      "permanent": true
    },
    {
      "source": "/index",
      "destination": "/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self' https: data:; script-src 'self' 'unsafe-inline' 'unsafe-eval' https:; style-src 'self' 'unsafe-inline' https:; img-src 'self' data: https: blob:; font-src 'self' https: data:; connect-src 'self' https:; media-src 'self' https: blob:; frame-src 'self' https:;"
        }
      ]
    },
    {
      "source": "/assets/(.*).css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ]
    },
    {
      "source": "/assets/(.*).js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Encoding",
          "value": "gzip"
        }
      ]
    },
    {
      "source": "/assets/(.*).(jpg|jpeg|png|gif|ico|svg|webp|avif)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).htm",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).(json|xml|txt)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true,
  "public": false,
  "name": "quravel-streaming",
  "alias": ["quravel.vercel.app"],
  "scope": "sfo1",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "_site"
      }
    }
  ],
  "crons": [],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "preview": true
    }
  }
}